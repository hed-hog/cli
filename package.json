{
  "name": "@hedhog/cli",
  "version": "0.0.195",
  "bin": {
    "hedhog": "bin/hedhog.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node bin/hedhog.js",
    "dev": "tsc --watch",
    "prod": "ts-node ./scripts/deploy.ts",
    "postprod": "npm run clean",
    "clean": "gulp clean:bundle",
    "diff": "git diff --output diff.md",
    "project": "npm run clean && sh scripts/get_all_ts.sh . > project.md"
  },
  "keywords": [
    "hedhog",
    "cli",
    "command-line interface",
    "typescript",
    "nestjs",
    "developer tools",
    "module management",
    "database migrations",
    "scaffolding",
    "dynamic applications",
    "scalable applications",
    "automation",
    "backend tools",
    "project setup",
    "devops",
    "workflow optimization"
  ],
  "author": "https://github.com/hcodebr",
  "license": "MIT",
  "description": "The @hedhog/cli is a command-line interface designed to streamline development processes within the Hedhog ecosystem. It provides utilities for managing configurations, creating modules, handling database migrations, and scaffolding projects. The CLI is tailored for developers building scalable and dynamic applications, ensuring seamless integration with the Hedhog libraries and tools.",
  "dependencies": {
    "@commander-js/extra-typings": "^12.1.0",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^2.4.3",
    "chalk": "4.1.2",
    "cli-table3": "0.6.5",
    "commander": "^12.1.0",
    "ejs": "^3.1.10",
    "http": "^0.0.1-security",
    "inquirer": "8.2.6",
    "isomorphic-git": "^1.27.1",
    "mysql2": "^3.11.2",
    "node-emoji": "^2.1.3",
    "openai": "^4.71.0",
    "pg": "^8.12.0",
    "prettier": "^3.3.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "typescript": "^5.6.2",
    "yaml": "^2.6.0"
  },
  "devDependencies": {
    "@hedhog/utils": "^0.0.19",
    "@types/ejs": "^3.1.5",
    "@types/gulp": "^4.0.17",
    "@types/inquirer": "9.0.7",
    "@types/node": "^22.5.4",
    "@types/node-emoji": "^1.8.2",
    "@types/pg": "^8.11.9",
    "concurrently": "^9.0.0",
    "delete-empty": "3.0.0",
    "gulp": "5.0.0",
    "gulp-clean": "0.4.0",
    "sh": "^0.0.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "main": "gulpfile.js",
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hed-hog/cli.git"
  },
  "bugs": {
    "url": "https://github.com/hed-hog/cli/issues"
  },
  "homepage": "https://github.com/hed-hog/cli#readme"
